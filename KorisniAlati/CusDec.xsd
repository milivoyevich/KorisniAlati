											<xsd:documentation>Deklaracija - rubrika 1         Vrsta tranzita - III potpodela</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:length value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="PartiesInvolved" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ConsignorExporter" type="PartyRelatedInformation" minOccurs="0">
