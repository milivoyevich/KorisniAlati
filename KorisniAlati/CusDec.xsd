											<xsd:documentation>Posiljalac - izvoznik - rubrika 2</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="ConsigneeImporter" type="PartyRelatedInformation" minOccurs="0">
