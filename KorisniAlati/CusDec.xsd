											<xsd:documentation>Primalac - rubrika 8</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="DeclarantRepresentativ" type="PartyRelatedInformation">
										<xsd:annotation>
											<xsd:documentation>Deklarant - zastupnik - rubrika 14</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Forms" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Obrasci - ukupan broj obrazaca - rubrika 3</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:short">
									<xsd:minInclusive value="1"/>
									<xsd:maxInclusive value="334"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="TransportDocNumber" minOccurs="0">
