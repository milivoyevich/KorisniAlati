													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="PurposeOfImport" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Sifra namene uvoza</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="PreferentialRegime" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="TaxPreferentialRegimeCode">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="4"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="AccisePreferentialRegimeCode">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="4"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="GoodsItemMeasurements" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="GrossWeight">
										<xsd:annotation>
											<xsd:documentation>Bruto masa - rubrika 35</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:decimal">
												<xsd:totalDigits value="15"/>
												<xsd:fractionDigits value="2"/>
												<xsd:minInclusive value="0"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="Cube" type="Measurements" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Zapremina - za sada se ne popunjava</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="CustomsValue" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Cena robe - rubrika 42</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:totalDigits value="15"/>
									<xsd:fractionDigits value="2"/>
									<xsd:minInclusive value="0"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PreviousCustomsDocuments" type="PreviusCustomsDocumentsTypeTransit" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Sazeta deklaracija/prethodni dokument - rubrika 40</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="AdditionalDocumentInformation" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Rubrika 44</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="AdditionalDocument" maxOccurs="unbounded">
										<xsd:annotation>
											<xsd:documentation>Prateca dokumenta</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="Type">
													<xsd:annotation>
														<xsd:documentation>Tip prilozene isprave koji se uzima iz registra prilozenih isprava(sifarnik)</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="3"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Date" type="datum" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Datum izdavanja prilozene isprave ili neki referentni datum uz postupak</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="PartyCode" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>PIB uz prilozenu ispravu/uverenje/odobrenje</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:integer">
															<xsd:minInclusive value="000000001"/>
															<xsd:maxInclusive value="9999999999999"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="IssuerCode" type="CustomsOfficeCode" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Sifra organizacione jedinice Upreve Carina, koja je izdala ispravu</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="DeclarationType" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Vrsta carinske isprave vezane za dati dokument</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="2"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="DocumentIdNumber" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Broj prilozene isprave(Interni)</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:int">
															<xsd:minInclusive value="0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Year" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Godina prilozene isprave</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:short">
															<xsd:minInclusive value="0"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="Country" type="Country" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Sifra zemlje iz prilozene isprave</xsd:documentation>
													</xsd:annotation>
												</xsd:element>
												<xsd:element name="TypeOfDutyRegimeCoded" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Sifra zakonskog osnova za oslobodjenje iz prilozene isprave</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:length value="5"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="AdditionalInformation" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Upisuju se sadrzinski podaci koji su vezani za odabranu sifru (na pr. redni broj dokumenta, iznos prevoznih trosokova,...</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="0"/>
															<xsd:maxLength value="30"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="FinancialDetails" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Upisuje se iznos i sifra valute u prilozenoj ispravi</xsd:documentation>
													</xsd:annotation>
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="CurrencyCode">
																<xsd:annotation>
																	<xsd:documentation>Sifra valute u prilozenoj ispravi</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:string">
																		<xsd:length value="3"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
															<xsd:element name="MonetaryAmount">
																<xsd:annotation>
																	<xsd:documentation>Iznos u prilozenoj ispravi</xsd:documentation>
																</xsd:annotation>
																<xsd:simpleType>
																	<xsd:restriction base="xsd:decimal">
																		<xsd:totalDigits value="15"/>
																		<xsd:fractionDigits value="2"/>
																		<xsd:minInclusive value="0"/>
																	</xsd:restriction>
																</xsd:simpleType>
															</xsd:element>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="FinalProductNumber" minOccurs="0">
													<xsd:annotation>
														<xsd:documentation>Redni broj finalnog produkta iz odobrenja</xsd:documentation>
													</xsd:annotation>
													<xsd:simpleType>
														<xsd:restriction base="xsd:short">
															<xsd:minInclusive value="0"/>
															<xsd:maxInclusive value="999"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
											<xsd:attribute name="IDNo" use="required">
												<xsd:simpleType>
													<xsd:restriction base="xsd:int">
														<xsd:minInclusive value="1"/>
													</xsd:restriction>
												</xsd:simpleType>
											</xsd:attribute>
											<xsd:attribute name="ReferralIndicator" type="xsd:boolean" use="required"/>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="GoodsItemNumber" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:short">
								<xsd:minInclusive value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="CustomsOfficeOfDeclarationCode" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="5"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="DateOfArrivalAtFirstCustomsPort" type="datum" use="required"/>
		<xsd:attribute name="DocumentNumber" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="1"/>
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="VersionNumber" type="xsd:int" use="optional" default="0"/>
		<xsd:attribute name="OperationCode" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="INSERT"/>
					<xsd:enumeration value="UPDATE"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="ActionControlCode" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="ShipmentList" type="xsd:boolean" use="optional" default="0"/>
	</xsd:complexType>
	<xsd:complexType name="GuaranteeType">
		<xsd:sequence>
			<xsd:element name="ResponsibleCustomsOffice" type="CustomsOfficeCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Carinska ispostava koja je evidentirala garanciju ili obezbedjenje</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="YearOfIssueance" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Godina garancije  ili obezbedjenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NumberOfGuarantee" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Broj garancije  ili obezbedjenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="SecurityType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Vrsta obezbedjenja - rubrika 52</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="MonerayAmount" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Iznos obezbedjenja - rubrika B</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:fractionDigits value="2"/>
						<xsd:totalDigits value="15"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="PreviousCustomsDocumentTypeTransit">
		<xsd:sequence>
			<xsd:element name="ResponsibleCustomsOfficce" type="CustomsOfficeCode">
				<xsd:annotation>
					<xsd:documentation>Sifra carinske ispostave</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentCoded">
				<xsd:annotation>
					<xsd:documentation>Sifra vrste prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentNumber">
				<xsd:annotation>
					<xsd:documentation>Broj prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentYear">
				<xsd:annotation>
					<xsd:documentation>Godina prihvatanja prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentItemNo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Redni broj naimenovanja iz prethodne isprave po kojoj je roba bila prijavljena </xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="GrossWeight" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bruto masa razduzenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:totalDigits value="15"/>
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NumberOfPackages" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Broj koleta razduzenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="IDNo" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="1"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:element name="CusResMessage">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Header" type="HeaderType"/>
				<xsd:element name="Body">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="ErrorResponse" minOccurs="0" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="ErrorWeight" type="xsd:string"/>
										<xsd:element name="ErrorDescription" type="xsd:string"/>
									</xsd:sequence>
									<xsd:attribute name="AgentCode" type="xsd:string" use="required"/>
									<xsd:attribute name="ResponsibleCustomsOfficeCoded" type="xsd:string" use="required"/>
									<xsd:attribute name="DocumentSubmitionDate" type="xsd:dateTime" use="required"/>
									<xsd:attribute name="DocumentNumber" type="xsd:string" use="required"/>
									<xsd:attribute name="GoodsItemNumber" type="xsd:int" use="optional"/>
									<xsd:attribute name="VersionNumber" type="xsd:int" use="required"/>
									<xsd:attribute name="DeclarationBoxNumber" type="xsd:int" use="optional"/>
									<xsd:attribute name="IDNo" type="xsd:int" use="optional"/>
									<xsd:attribute name="MessageNumber" type="xsd:int" use="required"/>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Response" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:choice>
											<xsd:sequence>
												<xsd:element name="Code" type="xsd:string"/>
												<xsd:element name="Description" type="xsd:string"/>
											</xsd:sequence>
											<xsd:sequence>
												<xsd:element name="Token" type="xsd:string"/>
											</xsd:sequence>
										</xsd:choice>
									</xsd:sequence>
									<xsd:attribute name="DocumentNumber" type="xsd:string" use="required"/>
									<xsd:attribute name="AgentCode" type="xsd:string" use="required"/>
									<xsd:attribute name="ResponsiblePartyCode" use="required">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="13"/>
												<xsd:minLength value="9"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="DocumentSubmitionDate" type="datum" use="required"/>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="HeaderType">
		<xsd:sequence>
			<xsd:element name="SenderDetails">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IDAuthentication">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="SenderID" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Topic" type="xsd:string"/>
						<xsd:element name="Version" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SummDecType">
		<xsd:sequence>
			<xsd:element name="Header">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="PartiesInvolved">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ResponsibleParty">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="PartyCode">
													<xsd:simpleType>
														<xsd:restriction base="xsd:integer">
															<xsd:minInclusive value="000000001"/>
															<xsd:maxInclusive value="9999999999999"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="PartyNameAddress">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:minLength value="5"/>
															<xsd:maxLength value="60"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
												<xsd:element name="PartyDocumentNumber" minOccurs="0">
													<xsd:simpleType>
														<xsd:restriction base="xsd:string">
															<xsd:maxLength value="13"/>
														</xsd:restriction>
													</xsd:simpleType>
												</xsd:element>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Guarantee" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Garancija</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="Details" type="GuaranteeType" minOccurs="0"/>
									<xsd:element name="GuaranteePartyCode" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>PIB podnosioca garancije</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:integer">
												<xsd:minInclusive value="000000001"/>
												<xsd:maxInclusive value="9999999999999"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="Warehouse" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Rubrike 4 i 5</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="ResponsibleCustomsOffice" type="CustomsOfficeCode" minOccurs="0" maxOccurs="0"/>
									<xsd:element name="WarehouseCoded">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:minLength value="1"/>
												<xsd:maxLength value="5"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="WarehouseRefNumber" minOccurs="0">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="WarehousingDateTime" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="WarehousingDate" type="datum"/>
												<xsd:element name="WarehousingTime" type="xsd:time" minOccurs="0"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="TransportDocument">
							<xsd:annotation>
								<xsd:documentation>Prevozna Isprava (Rubrika 3)</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="TransportDocumentType">
										<xsd:annotation>
											<xsd:documentation>Tip prevozne isprave </xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="4"/>
												<xsd:minLength value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="TransportDocumentNumber">
										<xsd:annotation>
											<xsd:documentation>Broj prevozne isprave</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="AdditionalTransportData" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Dodatne informacije</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="TransportationDetails" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="RoadTransportationDetails" minOccurs="0" maxOccurs="999">
										<xsd:annotation>
											<xsd:documentation>Rubrika 2</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:complexContent>
												<xsd:extension base="MeanOfTransport">
													<xsd:attribute name="ID" use="required">
														<xsd:simpleType>
															<xsd:restriction base="xsd:int">
																<xsd:minInclusive value="0"/>
															</xsd:restriction>
														</xsd:simpleType>
													</xsd:attribute>
												</xsd:extension>
											</xsd:complexContent>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="TransportID" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Ime broda, oznaka leta ili voza(1 red rubrike 2)</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="30"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="DateOfFlight" type="datum" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Datum Leta (2 podela reda1 rubrike 1 u avionskom saobracaju)</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="Nationality" type="Country" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Nacionalnost prevoznog sredstva</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
								<xsd:attribute name="IsList" type="xsd:boolean" use="required"/>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="TypeOfTransportation">
							<xsd:annotation>
								<xsd:documentation>Sifra saobracaja na granici</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="2"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="GoodsItemData" maxOccurs="999">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ShipingMarks" type="ShipingMarks" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Rubrika 9</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="GoodsDescription" type="GoodsDescriptionType-Transit" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Opis robe (Rubrika 9)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CustomsGoodsType" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Vrsta carinske robe - rubrika 9</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:length value="1"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="GrossWeight" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Rubrika 10</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:decimal">
									<xsd:totalDigits value="15"/>
									<xsd:fractionDigits value="2"/>
									<xsd:minInclusive value="0"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="PreviousCustomsDocument" type="PreviousCustomsDocumentTypeTransit" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>PrethodnaIsprava (Rubrika 11)</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="SparePartIndicator" type="xsd:boolean" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Indikator kruzeceg dela - za avione</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Warehouse" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Identifikacija prostora za privremeni smestaj               (Rubrika 4)</xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="WarehouseCoded" type="xsd:int">
										<xsd:annotation>
											<xsd:documentation>Sifra prostora za privremeni smestaj </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="WarehouseRefNumber">
										<xsd:annotation>
											<xsd:documentation>Broj iz evidencije magacinske knjige</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="WarehousePosition" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Pozicija u skladista</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="TransportDocument" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Rubrika 8 </xsd:documentation>
							</xsd:annotation>
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="TransportDocumentType">
										<xsd:annotation>
											<xsd:documentation>Vrsta tovarnog lista                (Prva potpodela rubrike 8)</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="4"/>
												<xsd:minLength value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="TransportDocumentNo">
										<xsd:annotation>
											<xsd:documentation>Rubrika 8</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="TransportDocumentIndicator" type="xsd:boolean" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>Indikator tovarnog lista </xsd:documentation>
										</xsd:annotation>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="GoodsItemNumber" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:int">
								<xsd:minInclusive value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="CustomsOfficeOfDeclarationCode" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="5"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="DateOfArrivalAtFirstCustomsPort" type="datum" use="required"/>
		<xsd:attribute name="DocumentNumber" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:minLength value="1"/>
					<xsd:maxLength value="20"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="VersionNumber" type="xsd:int" use="optional" default="0"/>
		<xsd:attribute name="OperationCode" type="xsd:string" use="required"/>
		<xsd:attribute name="ActionControlCode" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:length value="2"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PreviousCustomsDocumentTypeClearence">
		<xsd:sequence>
			<xsd:element name="ResponsibleCustomsOffice" type="CustomsOfficeCode">
				<xsd:annotation>
					<xsd:documentation>Sifra carinske ispostave prethodnog dokumenta</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentCoded">
				<xsd:annotation>
					<xsd:documentation>Sifra vrste prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="2"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentNumber">
				<xsd:annotation>
					<xsd:documentation>Broj prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsDocumentYear">
				<xsd:annotation>
					<xsd:documentation>Godina prihvatanja prethodne isprave</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="PreviousCustomsItemDocumentNo" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Redni broj naimenovanja iz prethodne isprave po kojoj je roba bila prijavljena</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:short">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="GrossWeight" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Bruto masa razduzenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:decimal">
						<xsd:totalDigits value="15"/>
						<xsd:fractionDigits value="2"/>
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="NumberOfPackages" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Broj koleta razduzenja</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:minInclusive value="0"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="DischargeOfGoods" type="Measurements" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Kolicina Razduzenja</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="IDNo" use="required">
			<xsd:simpleType>
				<xsd:restriction base="xsd:int">
					<xsd:minInclusive value="1"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:complexType name="PreviousCustomsDocumentsTypeClearence">
		<xsd:sequence>
			<xsd:element name="PreviousCustomsDocument" type="PreviousCustomsDocumentTypeClearence" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="datum">
		<xsd:restriction base="xsd:date">
			<xsd:minInclusive value="1900-01-01"/>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
