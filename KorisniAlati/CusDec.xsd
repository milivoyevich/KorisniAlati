								<xsd:documentation>Zemlja otpreme-rubrika 15</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CountryOfOrigin" type="Country" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>Zemlja porekla</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="DeliveryDestination" type="Country" minOccurs="0">
