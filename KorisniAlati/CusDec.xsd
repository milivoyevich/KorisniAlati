								<xsd:documentation>Ukupan broj naimenovanja - rubrika 5</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:short">
									<xsd:minInclusive value="1"/>
									<xsd:maxInclusive value="999"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="TotalNumberOfPackages" minOccurs="0">
